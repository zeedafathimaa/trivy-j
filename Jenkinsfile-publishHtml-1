pipeline {
  agent any
  options {
    buildDiscarder(logRotator(numToKeepStr: '5'))
  }
  stages {
    stage('Build') {
      steps {
        sh 'docker build -t trivyj/java-web-app:latest .'
      }
    }
    stage('Scan') {
      steps{
        sh 'trivy trivyj/java-web-app:latest'
      }
    }

  }
}